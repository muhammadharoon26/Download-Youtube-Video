{
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python@3.1.0"
        }
    ],
    "functions": {
        "api/main.py": {
            "maxDuration": 60
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ]
}